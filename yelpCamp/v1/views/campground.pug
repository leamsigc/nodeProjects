include partials/header.pug
.container
    h1.text-center.my-5  Welcome to the Camp Ground
    div.row
        each camp in campGroundArr
            .col-md-4
                .card
                    img(src= camp.image, alt="" style="width:100%; height:300px;").card-img-top
                    .card-body
                        h5.card-title #{camp.name}
       
include partials/footer.pug